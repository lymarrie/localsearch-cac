        {
          "id": "c_primaryColor"
        },
        {
          "id": "c_secondaryColor"
        },
        {
          "id": "c_font"
        },
        {
          "id": "c_enableConversionTracking"
        },
        {
          "id": "c_googleAnalytics"
        },
        {
          "id": "c_APIKeys"
        },
        {
          "id": "c_relatedPages"
        },
        {
          "id": "slug"
        }
      ]
    },
    {
      "displayName": "Display Sections",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "c_displayPhotoGallery"
        },
        {
          "id": "c_displayStaticMap"
